<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### AUCTION AGENTS ### -->
	<agent name="auction-random">
		<set class-path="bin/"/>
		<set class-name="template.AuctionTemplate"/>
	</agent>

	<agent name="auction-main-76">
		<set class-path="bin/"/>
		<set class-name="Auction"/>
		<set discount-factor=".5"/>
		<set convergence-threshold="0.01"/>
		<set max-tasks="50"/>
	</agent>
	
	
</agents>
